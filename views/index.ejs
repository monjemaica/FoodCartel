                <form id="login-form">
                    <div class="email-cn">
                        <label for="email">
                            <i class="fa fa-envelope"></i> <b>Email</b>
                        </label>
                        <input type="email" placeholder="Enter Email" id="email" name="email" autocomplete="off" required>
                    </div>

                    <div class="pass-cn">
                        <label for="password">
                            <i class="fa fa-asterisk"></i> <b>Password</b>
                        </label>
                        <input type="password" placeholder="Enter Password" id="password" name="password" autocomplete="off" required>
                        <span class="status-message" style="font-size: 12px; font-weight: 600;">test</span>
                    </div>
                    <button type="submit">Login</button>

                    <div class="register">
                        <label for="signup">Not a member? <b><a href="./signup.html">Sign Up</a></b></label>
                    </div>
                </form>
            </div>
        </div>

        <button id="login-btn">Login <i style="font-size:24px" class="fa">&#xf107;</i></button>

        <script src="script.js"></script>

